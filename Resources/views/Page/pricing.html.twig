{% extends 'DuskUserBundle::layout.html.twig' %}

{% block maincontent %}
    {% block fos_user_content %}
<div class="banner about-dusk-banner">
    <div class="container">
        <div class="pricing-table">
            <div class="pricing-1 grid_3">
                <div class="pricing_item frist">
                    <div class="pricing_header">
                        {#<h3 class="pricing_title">Diamond</h3>#}
                        <div class="pricing_price_wrapper">
                            <h3 class="pricing_price">Free</h3>
                            <div class="pricing_timeline">14 Days</div>
                        </div>
                    </div>
                    <ul>
                        <li> Signup for a Free 2 Week Trial </li>
                        <li class="minheight80">Get free access to any one playlist of your choice for two weeks. Be streaming live in minutes. </li>
						<li>Email support</li>
						<li class="minheight40">&nbsp;</li>
                    </ul>
                    <div class="pricing_button"><a href="{{ path('fos_user_registration_register')}}" class="signup">Sign Up</a></div>
                </div>
                <div class="pricing_item">
                    <div class="pricing_header">
                        {#<h3 class="pricing_title">Platinum</h3>#}
                        <div class="pricing_price_wrapper">
                            <h3 class="pricing_price">$100</h3>
                            <div class="pricing_timeline">per month</div>
                        </div>
                    </div>
                    <ul>
                        <li> <strong>1</strong> Room </li>
                        <li class="minheight80">Access to any one playlist of your choice per month. </li>
						<li>Email support</li>
						<li class="minheight40"></li>
                    </ul>
                    <div class="pricing_button"> <a href="{{ path('fos_user_registration_register')}}" class="signup">Sign Up</a></div>
                </div>
                <div class="pricing_item last">
                    <div class="pricing_header">
                        {#<h3 class="pricing_title">Gold</h3>#}
                        <div class="pricing_price_wrapper">
                            <h3 class="pricing_price">$250</h3>
                            <div class="pricing_timeline">per month</div>
                        </div>
                    </div>
                    <ul>
                        <li> <strong>1</strong> Room </li>
                        <li class="minheight80">Unlimited Playlists </li>
                        <li>Email support</li>
                        <li class="minheight40"><strong>$125 </strong> per extra room p/m at any venue </li>

                    </ul>
                    <div class="pricing_button"> <a href="{{ path('fos_user_registration_register')}}" class="signup">Sign Up</a></div>
                </div>
            </div>

            

        </div>
    </div>
</div>

        {% block orangestrip %}
            {{ parent() }}
        {% endblock orangestrip %}

        {% block instantpreview %}
            {{ parent() }}
        {% endblock instantpreview %}

        {% block whoisdusk %}
            {{ parent() }}
        {% endblock %}
    {% endblock %}
{% endblock %}